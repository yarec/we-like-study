create or replace procedure init_basic is
flag NUMBER;
begin


      flag := 0;
      select count(*) into flag from all_tables where table_name=upper('standards');
      if (flag<>0) then
         execute immediate 'drop table standards';
      end if;
      execute immediate '
      create table standards (
           code             varchar(200) not null
          ,value            varchar(200) not null
          ,source           varchar(200) not null
          ,remark           varchar2(400) 
          
          ,txt1             varchar(200) 
          ,txt2             varchar(200) 
          ,txt3             varchar(200) 
          ,txt4             varchar(200) 
          
      )';
      execute immediate 'comment on table standards is                        ''标准'''; 
      
      execute immediate 'comment on column standards.code is                  ''编码''';
      execute immediate 'comment on column standards.value is                 ''值''';
      execute immediate 'comment on column standards.source is                ''来源''';
      execute immediate 'comment on column standards.remark is                ''批注''';
      

      
      execute immediate 'PURGE RECYCLEBIN';
      
